<% @photobooth_images = ["http://imgur.com/nYeCZFM", "http://imgur.com/QZRHQMP", "http://imgur.com/VS8G66t"] %>
<!-- carosel -->	

<div class="carousel-container">
<div id="carousel-1" class="carousel slide">
	<!-- Indicators -->
	<ol class="carousel-indicators">
	<li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
	<li data-target="#carousel-example-generic" data-slide-to="1"></li>
	<li data-target="#carousel-example-generic" data-slide-to="2"></li>
	</ol>

	<!-- Wrapper for slides -->
	<div class="carousel-inner">
	<div class="item active">
		<img src="http://imgur.com/fRwDGJF.jpg" alt="...">
		<div class="carousel-caption">
		...
		</div>
	</div>
	<% @photobooth_images.each do |image| %>
	<div class="item">
		<img src="<%= image %>.jpg" alt="...">
		<div class="carousel-caption">
		...
		</div>
	</div>
	<% end %>
	</div>

	<!-- Controls -->
	<a class="carousel-control"  href="#carousel-1" data-slide="prev">
	<span class="icon-prev"></span>
	</a>
	<a class="carousel-control" id="carasoul-control-right" href="#carousel-1" data-slide="next">
	<span class="icon-next"></span>
	</a>
</div>
</div>


<!-- end carosel -->