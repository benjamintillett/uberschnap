<!-- carosel -->	
<div class="col-md-4">
<div class="carousel-container">
<div id="carousel-example-generic" class="carousel slide">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
    <li data-target="#carousel-example-generic" data-slide-to="1"></li>
    <li data-target="#carousel-example-generic" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="http://i1370.photobucket.com/albums/ag278/benjamin_tillett/Unknown/2013_09_12_205845_zpsf7d7f073.jpg" alt="http://placehold.it/272x407">
      <div class="carousel-caption">
        ...
      </div>
    </div>
    <% @unknown_images.each do |image| %>
    <div class="item">
      <img src="<%= image %>.jpg" alt="http://placehold.it/272x407">
      <div class="carousel-caption">
        ...
      </div>
    </div>
    <% end %>
    <% @wedding_images.each do |image| %>
    <div class="item">
      <img src="<%= image %>.jpg" alt="http://placehold.it/272x407">
      <div class="carousel-caption">
        ...
      </div>
    </div>
    <% end %>
    ...
  </div>

  <!-- Controls -->
  <a class="carousel-control"  href="#carousel-example-generic" data-slide="prev">
    <span class="icon-prev"></span>
  </a>
  <a class="carousel-control" id="carasoul-control-right" href="#carousel-example-generic" data-slide="next">
    <span class="icon-next"></span>
  </a>
</div>
</div>
