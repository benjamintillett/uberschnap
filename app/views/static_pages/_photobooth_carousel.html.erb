<% @photobooth_images = ["http://imgur.com/UXO2Mk4", "http://imgur.com/uX3GEOa", "http://imgur.com/UzY8mgE", "http://imgur.com/XPJbtlN", "http://imgur.com/fRwDGJF", "http://imgur.com/8MtFFHe", "http://imgur.com/R4SxoX1", "http://imgur.com/3KhNkV7", "http://imgur.com/sIs0RKT", "http://imgur.com/rXCkCOD", "http://imgur.com/76LpKM4", "http://imgur.com/t6LARH7", "http://imgur.com/boiKCC6", "http://imgur.com/vl1wSAZ", "http://imgur.com/f0Vxyvb", "http://imgur.com/zuzbsqD"] %>
<!-- carosel -->	

<div class="carousel-container">
<div id="carousel-1" class="carousel slide">
	<!-- Indicators -->
	<ol class="carousel-indicators">
	<li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
	<li data-target="#carousel-example-generic" data-slide-to="1"></li>
	<li data-target="#carousel-example-generic" data-slide-to="2"></li>
	</ol>

	<!-- Wrapper for slides -->
	<div class="carousel-inner">
	<div class="item active">
		<img src="http://imgur.com/fRwDGJF.jpg" alt="...">
		<div class="carousel-caption">
		...
		</div>
	</div>
	<% @photobooth_images.each do |image| %>
	<div class="item">
		<img src="<%= image %>.jpg" alt="...">
		<div class="carousel-caption">
		...
		</div>
	</div>
	<% end %>
	</div>

	<!-- Controls -->
	<a class="carousel-control"  href="#carousel-1" data-slide="prev">
	<span class="icon-prev"></span>
	</a>
	<a class="carousel-control" id="carasoul-control-right" href="#carousel-1" data-slide="next">
	<span class="icon-next"></span>
	</a>
</div>
</div>


<!-- end carosel -->